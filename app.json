{
  "expo": {
    "name": "flappy-bird-RN",
    "slug": "flappy-bird-RN",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/sun.gif",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/sun.gif",
        "backgroundColor": "#000000"
      },
      "package": "com.thierry.santos.io.flappybirdRN"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
